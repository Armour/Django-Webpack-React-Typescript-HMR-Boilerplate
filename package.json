  "scripts": {
    "build-dll": "webpack --color --config webpack.config.dll.babel.js",
    "build-dev": "webpack-dev-server --color --config webpack.config.dev.babel.js",
    "build-prod": "webpack --color --config webpack.config.prod.babel.js",
    "test": "jest --no-cache --coverage --runInBand",
    "coveralls": "cat ./coverage/remapped/lcov.info | ./node_modules/.bin/coveralls",
    "precommit": "yarn test",
    "prepush": "yarn test"
  },
