{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "rules": {
    "indent": [2, 2],
    "global-require": 0,
    "no-console": 0,
    "max-len": 0,
    "import/export": 2,
    "import/first": 2,
    "import/no-deprecated": 1,
    "import/no-duplicates": 2,
    "import/no-unresolved": 2,
    "import/no-webpack-loader-syntax": 1
  }
}
